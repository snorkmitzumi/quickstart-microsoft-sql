            ]
        },
        "ThirdAzIsFullNode": {
            "Fn::Equals": [
                {
                    "Ref": "ThirdAZ"
                },
                "full"
            ]
        },
        "IsTwoNode": {
            "Fn::Not": [
                {
                    "Fn::Equals": [
                        {
                            "Ref": "ThirdAZ"
                        },
